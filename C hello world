#include <stdio.h>
int main ()
{
   //Fazer um comentario.         _.       a funcao \n serve para informar ao compilador que deve pular uma linha onde  \n for colocado. segue no proximo exemplo.
   
   /*

      abrir / barra. * e fechar * barra e tbm
      uma forma de fazer
      comentario dentro do
      codigo    a difreneca entre barra barra
      e barras com * e que na primeira opcao eu
      escrevo comentarios curtos apenas
      em uma linha e a segunda oocau eu
      escrevo comentarios miores em mais linhas

    */
    printf (" 42 lisboa.\n");

    return 0;
    
    
    
    
    
    EXEMPLO DO \N
    
    #include <stdio.h>
int main ()
{
   //Fazer um comentario

    /*

      abrir / barra. * e fechar * barra e tbm
      uma forma de fazer
      comentario dentro do
      codigo    a difreneca entre barra barra
      e barras com * e que na primeira opcao eu
      escrevo comentarios curtos apenas
      em uma linha e a segunda oocau eu
      escrevo comentarios miores em mais linhas

    */
    printf (" 42 \n lisboa.\n");

    return 0;

}
